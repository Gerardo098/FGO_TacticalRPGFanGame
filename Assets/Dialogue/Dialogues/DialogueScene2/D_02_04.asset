  sequence:
  - {fileID: 11400000, guid: 4b1768b1b126d4e499bc6e906617feb0, type: 2}
  dialogue:
  - actor: {fileID: 11400000, guid: f01954c4a89cc0d41ae4823d2744271a, type: 2}
    Narrator: 0
    PortraitPosition: 0
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Why?"'
  - actor: {fileID: 11400000, guid: 8345a30b8e24fd94c862b7a75b0f3f4d, type: 2}
    Narrator: 0
    PortraitPosition: 1
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Just an experiment. I think I have an idea who we''re dealing with here.
      The more lovey-dovey, the better."'
  - actor: {fileID: 11400000, guid: f01954c4a89cc0d41ae4823d2744271a, type: 2}
    Narrator: 0
    PortraitPosition: 1
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"I hope you''re joking."'
  - actor: {fileID: 11400000, guid: 8345a30b8e24fd94c862b7a75b0f3f4d, type: 2}
    Narrator: 0
    PortraitPosition: 1
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Never been more serious in my life."'
  - actor: {fileID: 0}
    Narrator: 1
    PortraitPosition: 0
    layer: 0
    ErasePortraits: 1
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: You sigh, and look down at the woman that's still happily got a hold of
      you.
  - actor: {fileID: 11400000, guid: f01954c4a89cc0d41ae4823d2744271a, type: 2}
    Narrator: 0
    PortraitPosition: 0
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"...Kiyohime?"'
  - actor: {fileID: 0}
    Narrator: 1
    PortraitPosition: 0
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: The woman perks up like an energetic puppy and looks you dead in the eyes.
  - actor: {fileID: 11400000, guid: f2146d0d405016848aec264e10832182, type: 2}
    Narrator: 0
    PortraitPosition: 1
    layer: 0
    ErasePortraits: 1
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Ah! You do remember me! Of course you would, Anchin-sama!"'
  - actor: {fileID: 11400000, guid: f2146d0d405016848aec264e10832182, type: 2}
    Narrator: 0
    PortraitPosition: 0
    layer: 0
    ErasePortraits: 1
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"What would you like, Anchin-sama? Please, ask me anything! Know that
      you may speak to me about whatever you would fancy!"'
  - actor: {fileID: 11400000, guid: 8345a30b8e24fd94c862b7a75b0f3f4d, type: 2}
    Narrator: 0
    PortraitPosition: 2
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Bingo! I thought I''d recognize the name!"'
  - actor: {fileID: 11400000, guid: f01954c4a89cc0d41ae4823d2744271a, type: 2}
    Narrator: 0
    PortraitPosition: 2
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"Doctor, please explain."'
  - actor: {fileID: 11400000, guid: 8345a30b8e24fd94c862b7a75b0f3f4d, type: 2}
    Narrator: 0
    PortraitPosition: 2
    layer: 0
    ErasePortraits: 0
    background: {fileID: 0}
    FadeBGIn: 0
    EraseBG: 0
    text: '"John, allow me to introduce you to the titular character of a story from
      Japanese folklore - Kiyohime Densetsu."'
